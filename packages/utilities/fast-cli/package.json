    "@types/mocha": "^7.0.2",
    "@types/node": "^10",
    "@microsoft/eslint-config-fast-dna": "^1.2.0",
    "@types/lodash-es": "^4.17.4",
    "babel-core": "^7.0.0-bridge.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "globby": "^10",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^16.2.2",
